parameters:
    level: 6
    paths:
        - app/
        - bootstrap/
        - config/
        - database/
        - tests/
    excludePaths:
        - bootstrap/cache/
    ignoreErrors:
      - '/Property App\\Module\\[a-zA-Z0-9_]+\\Domain\\Model\\[a-zA-Z0-9_]+::\$[a-zA-Z0-9_]+ is unused\./'
      - '/Prettus\\Repository/'
